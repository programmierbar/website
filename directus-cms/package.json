  "dependencies": {
    "@slack/web-api": "^6.5.1",
    "axios": "^0.24.0",
    "directus": "9.16.0",
    "jsonwebtoken": "^8.5.1",
    "shared-code": "0.0.0"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
